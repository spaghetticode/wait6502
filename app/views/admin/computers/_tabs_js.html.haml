- content_for :head do
  = javascript_include_tag 'jquery-1.3.2.min'
  %script{:type => 'text/javascript'}
    $(function(){
    tabs = $('a.tab');
    divs = $('div.section');
    tabs.click(function(){
    divs.hide().filter(this.hash).show();
    return false;
    }).filter(':first').click();
    })