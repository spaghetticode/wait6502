<h1>Editing builtin_storage</h1>

<% form_for(@builtin_storage, :url => admin_builtin_storage_path(@builtin_storage)) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :storage_name_id %><br />
    <%= f.select :storage_name_id, StorageName.ordered.map(&:name) %>
  </p>
  <p>
    <%= f.label :storage_format_id %><br />
    <%= f.select :storage_format_id, StorageFormat.ordered.map(&:name) %>
  </p>
  <p>
    <%= f.label :storage_size_id %><br />
    <%= f.select :storage_size_id, StorageSize.ordered.map(&:name) %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', admin_builtin_storages_path %>