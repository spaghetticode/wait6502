<h1>Editing io_port</h1>

<% form_for(@io_port) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :connector %><br />
    <%= f.text_field :connector %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @io_port %> |
<%= link_to 'Back', io_ports_path %>